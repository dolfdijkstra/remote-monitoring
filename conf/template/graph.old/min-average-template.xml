<rrd_graph_def>
  <filename>${imagefile}</filename>
  <span>
    <start>${intervalstart}</start>
    <end>${intervalend}</end>
  </span>
  <options>
    <anti_aliasing>true</anti_aliasing>
    <title>${imagetitle}</title>
    <width>600</width>
    <height>200</height>
    <units_exponent>0</units_exponent>
    <rigid>true</rigid>
    <logarithmic>${logarithmic}</logarithmic>
    <min_value>0</min_value>
  </options>

  <datasources>
    <def>
      <name>average-avg</name>
      <rrd>${rrdfile}</rrd>
      <source>average</source>
      <cf>AVERAGE</cf>
    </def>
    <def>
      <name>average-min</name>
      <rrd>${rrdfile}</rrd>
      <source>average</source>
      <cf>MIN</cf>
    </def>
    <def>
      <name>average-max</name>
      <rrd>${rrdfile}</rrd>
      <source>average</source>
      <cf>MAX</cf>
    </def>
    <def>
      <name>average-last</name>
      <rrd>${rrdfile}</rrd>
      <source>average</source>
      <cf>LAST</cf>
    </def>

    <def>
      <name>min-avg</name>
      <rrd>${rrdfile}</rrd>
      <source>min</source>
      <cf>AVERAGE</cf>
    </def>
    <def>
      <name>min-min</name>
      <rrd>${rrdfile}</rrd>
      <source>min</source>
      <cf>MIN</cf>
    </def>
    <def>
      <name>min-max</name>
      <rrd>${rrdfile}</rrd>
      <source>min</source>
      <cf>MAX</cf>
    </def>
    <def>
      <name>min-last</name>
      <rrd>${rrdfile}</rrd>
      <source>min</source>
      <cf>LAST</cf>
    </def>

  </datasources>
  <graph>
    <area>
      <datasource>average-avg</datasource>
      <color>#00CC00</color>
      <legend>Average time (ms)</legend>
    </area>
    <gprint>
      <datasource>average-avg</datasource>
      <cf>AVERAGE</cf>
      <format>Avg = %10.3f</format>
    </gprint>
    <gprint>
      <datasource>average-max</datasource>
      <cf>MAX</cf>
      <format>Max = %10.3f</format>
    </gprint>
    <gprint>
      <datasource>average-last</datasource>
      <cf>LAST</cf>
      <format>Current = %10.3f\r</format>
    </gprint>
    <line>
      <datasource>average-avg</datasource>
      <color>#00AA00</color>
    </line>

    <area>
      <datasource>min-avg</datasource>
      <color>#3D64FF</color>
      <legend>Minimum time (ms)</legend>
    </area>
    <gprint>
      <datasource>min-avg</datasource>
      <cf>AVERAGE</cf>
      <format>Avg = %10.3f</format>
    </gprint>
    <gprint>
      <datasource>min-max</datasource>
      <cf>MAX</cf>
      <format>Max = %10.3f</format>
    </gprint>
    <gprint>
      <datasource>min-last</datasource>
      <cf>LAST</cf>
      <format>Current = %10.3f\r</format>
    </gprint>
     <line>
      <datasource>min-avg</datasource>
      <color>#3D44FF</color>
    </line>

    <comment>\r</comment>
    <comment>${generatedstamp}</comment>
    <comment>Performance results provided by JETM (http://jetm.void.fm)\r</comment>

  </graph>
</rrd_graph_def>