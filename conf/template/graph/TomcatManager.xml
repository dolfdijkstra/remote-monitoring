<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<rrd_graph_def>
    <filename>${imagefile}</filename>
    <span>
        <start>${intervalstart}</start>
        <end>${intervalend}</end>
    </span>
    <options>
        <title>${imagetitle}</title>
        <width>800</width>
        <height>200</height>
        <rigid>false</rigid>
        <logarithmic>${logarithmic}</logarithmic>
        <min_value>0</min_value>
        <image_format>png</image_format>
    </options>
    <datasources>
        <def>
            <name>line0</name>
            <rrd>${rrdfile}</rrd>
            <source>activeSessions</source>
            <cf>AVERAGE</cf>
        </def>
        <def>
            <name>line1</name>
            <rrd>${rrdfile}</rrd>
            <source>sessionCounter</source>
            <cf>AVERAGE</cf>
        </def>
        <def>
            <name>line2</name>
            <rrd>${rrdfile}</rrd>
            <source>maxActive</source>
            <cf>AVERAGE</cf>
        </def>
        <def>
            <name>line3</name>
            <rrd>${rrdfile}</rrd>
            <source>sessionAverageAli...</source>
            <cf>AVERAGE</cf>
        </def>
    </datasources>
    <graph>
        <line>
            <datasource>line0</datasource>
            <color>#ff0000</color>
            <legend>activeSessions</legend>
            <width>2</width>
        </line>
        <line>
            <datasource>line1</datasource>
            <color>#0000ff</color>
            <legend>sessionCounter</legend>
            <width>2</width>
        </line>
        <line>
            <datasource>line2</datasource>
            <color>#00ff00</color>
            <legend>maxActive</legend>
            <width>2</width>
        </line>
        <line>
            <datasource>line3</datasource>
            <color>#00ffff</color>
            <legend>sessionAverageAliveTime</legend>
            <width>2</width>
        </line>
    </graph>
</rrd_graph_def>
