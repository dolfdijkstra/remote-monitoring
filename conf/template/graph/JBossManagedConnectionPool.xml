<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<rrd_graph_def>
    <filename>${imagefile}</filename>
    <span>
        <start>${intervalstart}</start>
        <end>${intervalend}</end>
    </span>
    <options>
        <title>${imagetitle}</title>
        <width>600</width>
        <height>200</height>
        <units_exponent>0</units_exponent>
        <rigid>true</rigid>
        <logarithmic>${logarithmic}</logarithmic>
        <min_value>0</min_value>
        <image_format>png</image_format>
        <show_signature>false</show_signature>
    </options>
    <datasources>
        <def>
            <name>line0</name>
            <rrd>${rrdfile}</rrd>
            <source>AvailableConnecti...</source>
            <cf>AVERAGE</cf>
        </def>
        <def>
            <name>line1</name>
            <rrd>${rrdfile}</rrd>
            <source>ConnectionCount</source>
            <cf>AVERAGE</cf>
        </def>
        <def>
            <name>line2</name>
            <rrd>${rrdfile}</rrd>
            <source>MaxConnectionsInU...</source>
            <cf>AVERAGE</cf>
        </def>
        <def>
            <name>line3</name>
            <rrd>${rrdfile}</rrd>
            <source>InUseConnectionCount</source>
            <cf>AVERAGE</cf>
        </def>
    </datasources>
    <graph>
        <line>
            <datasource>line0</datasource>
            <color>#ff0000</color>
            <legend>AvailableConnectionCount</legend>
            <width>2</width>
        </line>

        <line>
            <datasource>line3</datasource>
            <color>#00ffff</color>
            <legend>InUseConnectionCount</legend>
            <width>2</width>
        </line>
        <line>
            <datasource>line1</datasource>
            <color>#0000ff</color>
            <legend>ConnectionCount</legend>
            <width>2</width>
        </line>
        <line>
            <datasource>line2</datasource>
            <color>#00ff00</color>
            <legend>MaxConnectionsInUseCount</legend>
            <width>2</width>
        </line>
    </graph>
</rrd_graph_def>
