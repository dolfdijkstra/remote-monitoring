<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<rrd_graph_def>
    <filename>${imagefile}</filename>
    <span>
        <start>${intervalstart}</start>
        <end>${intervalend}</end>
    </span>
    <options>
        <title>${imagetitle} - Memory</title>
        <width>800</width>
        <height>200</height>
        <rigid>false</rigid>
        <logarithmic>${logarithmic}</logarithmic>
        <min_value>0</min_value>
        <image_format>png</image_format>
        <alt_y_grid>true</alt_y_grid>
    </options>
    <datasources>
        <def>
            <name>line1</name>
            <rrd>${rrdfile}</rrd>
            <source>CommittedVirtualM...</source>
            <cf>AVERAGE</cf>
        </def>
        <def>
            <name>line2</name>
            <rrd>${rrdfile}</rrd>
            <source>FreePhysicalMemor...</source>
            <cf>AVERAGE</cf>
        </def>
        <def>
            <name>line3</name>
            <rrd>${rrdfile}</rrd>
            <source>FreeSwapSpaceSize</source>
            <cf>AVERAGE</cf>
        </def>
        <def>
            <name>line4</name>
            <rrd>${rrdfile}</rrd>
            <source>ProcessCpuTime</source>
            <cf>AVERAGE</cf>
        </def>
        <def>
            <name>line5</name>
            <rrd>${rrdfile}</rrd>
            <source>TotalPhysicalMemo...</source>
            <cf>AVERAGE</cf>
        </def>
        <def>
            <name>line6</name>
            <rrd>${rrdfile}</rrd>
            <source>TotalSwapSpaceSize</source>
            <cf>AVERAGE</cf>
        </def>
         
    </datasources>
    <graph>
        <line>
            <datasource>line1</datasource>
            <color>#0000ff</color>
            <legend>CommittedVirtualMemorySize</legend>
            <width>2</width>
        </line>
        <line>
            <datasource>line2</datasource>
            <color>#00ff00</color>
            <legend>FreePhysicalMemorySize</legend>
            <width>2</width>
        </line>

        <line>
            <datasource>line5</datasource>
            <color>#ffc800</color>
            <legend>TotalPhysicalMemorySize</legend>
            <width>2</width>
        </line>

        <line>
            <datasource>line6</datasource>
            <color>#ffafaf</color>
            <legend>TotalSwapSpaceSize</legend>
            <width>2</width>
        </line>
        <line>
            <datasource>line3</datasource>
            <color>#00ffff</color>
            <legend>FreeSwapSpaceSize</legend>
            <width>2</width>
        </line>
        <!--
        <line>
            <datasource>line4</datasource>
            <color>#ff00ff</color>
            <legend>ProcessCpuTime</legend>
            <width>2</width>
        </line>
         -->
    </graph>
</rrd_graph_def>
