<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<rrd_graph_def>
    <filename>${imagefile}</filename>
    <span>
        <start>${intervalstart}</start>
        <end>${intervalend}</end>
    </span>
    <options>
        <title>${imagetitle}</title>
        <width>800</width>
        <height>200</height>
        <rigid>false</rigid>
        <logarithmic>${logarithmic}</logarithmic>
        <min_value>0</min_value>
        <image_format>png</image_format>
    </options>
    <datasources>
        <def>
            <name>hits-avg</name>
            <rrd>${rrdfile}</rrd>
            <source>Hits</source>
            <cf>AVERAGE</cf>
        </def>
        <def>
            <name>line1</name>
            <rrd>${rrdfile}</rrd>
            <source>Misses</source>
            <cf>AVERAGE</cf>
        </def>
        <def>
            <name>line2</name>
            <rrd>${rrdfile}</rrd>
            <source>Size</source>
            <cf>AVERAGE</cf>
        </def>
        <def>
            <name>line3</name>
            <rrd>${rrdfile}</rrd>
            <source>ClearCount</source>
            <cf>AVERAGE</cf>
        </def>
        <def>
            <name>line4</name>
            <rrd>${rrdfile}</rrd>
            <source>RemoveCount</source>
            <cf>AVERAGE</cf>
        </def>
    </datasources>
    <graph>
        <line>
            <datasource>hits-avg</datasource>
            <color>#ff0000</color>
            <legend>Hits</legend>
            <width>2</width>
        </line>
        <line>
            <datasource>line1</datasource>
            <color>#0000ff</color>
            <legend>Misses</legend>
            <width>2</width>
        </line>
        <line>
            <datasource>line2</datasource>
            <color>#00ff00</color>
            <legend>Size</legend>
            <width>2</width>
        </line>
        <line>
            <datasource>line3</datasource>
            <color>#00ffff</color>
            <legend>ClearCount</legend>
            <width>2</width>
        </line>
        <line>
            <datasource>line4</datasource>
            <color>#ff00ff</color>
            <legend>RemoveCount</legend>
            <width>2</width>
        </line>

    </graph>
</rrd_graph_def>
