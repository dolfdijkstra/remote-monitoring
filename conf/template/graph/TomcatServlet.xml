<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<rrd_graph_def>
    <filename>${imagefile}</filename>
    <span>
        <start>${intervalstart}</start>
        <end>${intervalend}</end>
    </span>
    <options>
        <title>${imagetitle}</title>
        <width>800</width>
        <height>200</height>
        <rigid>false</rigid>
        <logarithmic>${logarithmic}</logarithmic>
        <min_value>0</min_value>
        <image_format>png</image_format>
    </options>
    <datasources>
        <def>
            <name>line0</name>
            <rrd>${rrdfile}</rrd>
            <source>processingTime</source>
            <cf>AVERAGE</cf>
        </def>
        <def>
            <name>line1</name>
            <rrd>${rrdfile}</rrd>
            <source>maxTime</source>
            <cf>AVERAGE</cf>
        </def>
        <def>
            <name>line2</name>
            <rrd>${rrdfile}</rrd>
            <source>minTime</source>
            <cf>AVERAGE</cf>
        </def>
        <def>
            <name>line3</name>
            <rrd>${rrdfile}</rrd>
            <source>requestCount</source>
            <cf>AVERAGE</cf>
        </def>
        <def>
            <name>line4</name>
            <rrd>${rrdfile}</rrd>
            <source>errorCount</source>
            <cf>AVERAGE</cf>
        </def>
                <def>
            <name>rc-last</name>
            <rrd>${rrdfile}</rrd>
            <source>requestCount</source>
            <cf>LAST</cf>
        </def>

        
    </datasources>
    <graph>
    <!-- 
        <line>
            <datasource>line0</datasource>
            <color>#ff0000</color>
            <legend>processingTime</legend>
            <width>2</width>
        </line>
        <line>
            <datasource>line1</datasource>
            <color>#0000ff</color>
            <legend>maxTime</legend>
            <width>2</width>
        </line>
        <line>
            <datasource>line2</datasource>
            <color>#00ff00</color>
            <legend>minTime</legend>
            <width>2</width>
        </line>
         -->
        <area>
            <datasource>line3</datasource>
            <color>#00ffff</color>
            <legend>requestCount</legend>
        </area>
        <line>
            <datasource>line4</datasource>
            <color>#ff0000</color>
            <legend>errorCount</legend>
            <width>2</width>
        </line>
        <gprint>
            <datasource>rc-last</datasource>
            <cf>LAST</cf>
            <format>Current = %10.3f %s</format>
    </gprint>

    </graph>
</rrd_graph_def>
