<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<factory>
    <query>*:type=Manager,*</query>
    <attributes>
        <attribute name="maxActive" type="GAUGE" />
        <attribute name="activeSessions" type="GAUGE" />
        <attribute name="sessionCounter" type="COUNTER" />
        <attribute name="sessionAverageAliveTime" type="GAUGE" />
    </attributes>
    <description-keys prefix="SessionManager">host,path</description-keys>
</factory>
