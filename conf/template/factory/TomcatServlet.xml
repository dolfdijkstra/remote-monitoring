<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<factory>
    <query>*:j2eeType=Servlet,*</query>
    <attributes>
        <attribute name="minTime" type="GAUGE" />
        <attribute name="processingTime" type="COUNTER" />
        <attribute name="errorCount" type="COUNTER" />
        <attribute name="maxTime" type="GAUGE" />
        <attribute name="requestCount" type="COUNTER" />
    </attributes>
    <description-keys prefix="Servlet">name,WebModule</description-keys>
</factory>
